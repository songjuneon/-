// 코드는 위에서부터 아래로 실행됨 자바스크립트는 html이 모두 동작 후 실행하는 경우가 많음 그래서 <body>태그 위에 작성해주는 경우가 대부분
<script> document.write('웹 화면에 표시되는 내용');</script>  //html에서 작성
<script src ="자바스크립트 파일명,js"></script> // 외부 첨부
/*
자바스크립트는 유연한 언어라서 명령어를 한줄만 쓰고 줄을 바꾸면 명령어를 제대로 인식할 수 있음, 단 한 줄에 2개이상을 쓸때에는 ; 붙여줘야 명령어가 끝났음을 인식함
var 변수 = "~" ,let 변수 = "`", const 변수 ="~" 
typeof 변수 //변수의 타입을 알수 있음
자료형은 string,number(int 정수, float실수) bool(true,fals) 가 있음
Math.random(); // 0이상1미만의 실수값 랜덤 출력
paraseInt();//소수점은 버리고 정수값으로 
.push(); // array의 마지막 인덱스 뒤에 값 추가 ex) var name= [ ];
cntrl x //잘라내기 단축키
for(시작; 끝; 증감){ 반복하고 싶은 명령어}; // Ex) for(var i =0; i<6; i++){ ~~};
if(조건){값이 true일 경우 실행할 명령어}; 
.indexOf() // 값이 이미 인덱스 안에  있다면 위치 값을 없으면 -1을 출력
.length() // 요소의 갯수 출력
while( 조건){}
.sort() //배열을 정렬함 (앞 자리의 수를 따름 ex 1, 11 ,2 
.sort((a,b)=>a-b) //오름차순 정리 b-a를 넣으면 내림차순
*/
